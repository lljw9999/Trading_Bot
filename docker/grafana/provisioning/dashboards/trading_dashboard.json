      "title": "Portfolio Overview",
      "type": "row",
      "collapsed": false,
      "panels": [
        {
          "title": "Portfolio Value",
          "type": "gauge",
          "datasource": "Prometheus",
          "targets": [
            {
              "expr": "trading_portfolio_value_dollars",
              "refId": "A"
            }
          ],
          "fieldConfig": {
            "defaults": {
              "thresholds": {
                "mode": "absolute",
                "steps": [
                  { "color": "red", "value": null },
                  { "color": "yellow", "value": 90000 },
                  { "color": "green", "value": 100000 }
                ]
              }
            }
          }
        },
        {
          "title": "P&L",
          "type": "stat",
          "datasource": "Prometheus",
          "targets": [
            {
              "expr": "sum(trading_pnl_realized_dollars)",
              "refId": "A"
            }
          ]
        }
      ]
    },
    {
